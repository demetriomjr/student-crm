{
  "welcomeFile": "/student.crm.ui/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/student.crm.ui/(.*)$",
      "target": "/student.crm.ui/$1",
      "localDir": "student.crm.ui",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/odata/v4/(.*)$",
      "target": "/$1",
      "destination": "srv-api",
      "authenticationType": "xsuaa",
      "csrfProtection": true
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api", 
      "authenticationType": "xsuaa",
      "csrfProtection": true
    }
  ]
}
